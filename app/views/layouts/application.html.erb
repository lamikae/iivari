<!DOCTYPE html>
<html>
<head>
  <meta charset=utf-8 />
  <title>Iivari</title>
  <%= stylesheet_link_tag "scaffold" %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "jquery-ui.min.js" %>
  <%= csrf_meta_tag %>
  <%= include_tiny_mce_if_needed %>
</head>
<body>
<% if current_user %>
<%= link_to t('links.logout'), user_session_path(current_user), :method => :delete %>
<% end %>


<%= yield %>

</body>
</html>
