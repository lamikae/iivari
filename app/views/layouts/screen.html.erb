<!DOCTYPE html>
<% if @cache == "true" %>
<html manifest="<%= "/" + params[:screen_key].to_s %>/screen.manifest<%= params[:resolution] ? "?resolution=" + params[:resolution].to_s : "" %>">
<% else %>
<html>
<% end %>
<head>
  <meta charset=utf-8 />
  <title>Iivari</title>

  <link href="/stylesheets/client.css" media="screen" rel="stylesheet" type="text/css" /> 
  <script src="/javascripts/jquery-1.4.2.min.js" type="text/javascript"></script> 
  <script src="/javascripts/jquery-ujs/src/rails.js" type="text/javascript"></script> 
  <%= csrf_meta_tag %>
</head>
<body>

  <%= yield %>

</body>
</html>
