<%# pass variables to client javascript (original slideshow) -%>
<% content_for :slideshow do -%>

	window.json_url = "<%= @json_url.html_safe %>";
	window.cache = <%= @cache %>;
	window.preview = <%= @preview %>;
	window.data_update_interval = <%= (Organisation.current.value_by_key('data_update_interval') || 60 * 60 * 24 ) * 1000 %>;

<% end %>
